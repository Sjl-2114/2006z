alue=\"index1\" v-model=\"t\"><span>{{item1}}</span><br>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<button @click=\"nextTest\" :disabled=\"dis\">{{add}}</button>\n\t\t\t\t<p v-show=\"isShow1\">{{num}}后进入下一题</p>\n\t\t\t</div>\n\t\t\t<div v-else>\n\t\t\t\t<span>没有题目了</span>\n\t\t\t\t<button @click=\"res\">再来一遍</button>\n\t\t\t</div>\n\t\t\t\n\t\t\t<div class=\"footer\">\n\t\t\t\t<span>共有{{currentNum}}/{{allNum}}道题,</span><span>答对：{{trueNum}}</span><span>答错:{{falseNum}}</span>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\n\timport data1 from \"../public/data.json\"\n\texport  default {\n\t\tname:\"test2\",\n\t\tdata(){\n\t\t\treturn {\n\t\t\t\tuserName:\"\",\n\t\t\t\tlogin:\"登录\",\n\t\t\t\tisShow:false,\n\t\t\t\tisUserShow:true,\n\t\t\t\tisMoShow:true,\n\t\t\t\tinfoList:data1.data,\n\t\t\t\tcurrentIndex:0,\n\t\t\t\tt:-1,\n\t\t\t\tadd:\"提交答案\",\n\t\t\t\tcurrentNum:1,\n\t\t\t\tallNum:data1.data.length,\n\t\t\t\ttrueNum:0,\n\t\t\t\tfalseNum:0,\n\t\t\t\tnum:3,\n\t\t\t\tisShow1:false,\n\t\t\t\tdis:false,\n\t\t\t\tisShow2:true\n\t\t\t}\n\t\t},\n\t\tmethods:{\n\t\t\tlogin1(){\n\t\t\t\tif(this.login == \"登录\"){\n\t\t\t\t\tlet reg = /^\\w{6,18}$/\n\t\t\t\t\tlet flag = reg.test(this.userName)\n\t\t\t\t\tif(flag){\n\t\t\t\t\t\tthis.isMoShow = false\n\t\t\t\t\t\tthis.login = \"退出\"\n\t\t\t\t\t\tthis.isShow = false\n\t\t\t\t\t\tthis.isUserShow = false\n\t\t\t\t\t}else{\n\t\t\t\t\t\tthis.isShow = true\t\t\t\t\t\t\n\t\t\t\t\t}\n\t\t\t\t}else{\n\t\t\t\t\tthis.login = \"登录\"\n\t\t\t\t\tthis.isMoShow = true\n\t\t\t\t\tthis.isUserShow = true\n\t\t\t\t\tthis.userName = \"\"\n\t\t\t\t\tthis.isShow2 = true\n\t\t\t\t\tthis.currentIndex = 0\n\t\t\t\t\tthis.currentNum = 1\n\t\t\t\t\tthis.trueNum = 0\n\t\t\t\t\tthis.falseNum = 0\n\t\t\t\t}\n\t\t\t\t\n\t\t\t},\n\t\t\tnextTest(){\n\t\t\t\tif(this.infoList[this.currentIndex].right == this.t){\n\t\t\t\t\tthis.trueNum++ \n\t\t\t\t\tthis.add = \"回答正确\"\n\t\t\t\t}else{\n\t\t\t\t\tthis.falseNum++\n\t\t\t\t\tthis.add = \"回答错误\"\n\t\t\t\t}\n\t\t\t\tthis.dis = true\n\t\t\t\tthis.isShow1 = true\n\t\t\t\tlet timer = setInterval(()=>{\t\t\t\t\t\n\t\t\t\t\tthis.num--\n\t\t\t\t\tif(this.num == 0){\n\t\t\t\t\t\tthis.dis = false\n\t\t\t\t\t\tthis.isShow1 = false\n\t\t\t\t\t\tthis.num = 3\n\t\t\t\t\t\tthis.add = \"提交答案\"\n\t\t\t\t\t\tclearInterval(timer)\n\t\t\t\t\t\tthis.currentIndex++\n\t\t\t\t\t\tthis.currentNum++\n\t\t\t\t\t\tthis.t = -1\n\t\t\t\t\t\tif(this.currentIndex == this.infoList.length){\n\t\t\t\t\t\t\tthis.isShow2 = false\n\t\t\t\t\t\t}\t\n\t\t\t\t\t}\t\t\t\t\t\n\t\t\t\t},1000)\n\t\t\t\t\t\n\t\t\t},\n\t\t\tres(){\n\t\t\t\tthis.isShow2 = true\n\t\t\t\tthis.currentIndex = 0\n\t\t\t\tthis.currentNum = 1\n\t\t\t\tthis.trueNum = 0\n\t\t\t\tthis.falseNum = 0\t\t\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style>\n\t#left{\n\t\twidth: 40%;\n\t\tfloat: left;\n\t\theight: 400px;\n\t\tborder: 1px  solid #ccc;\n\t}\n\t#right{\n\t\tposition: relative;\n\t\twidth: 40%;\n\t\tfloat: left;\n\t\theight: 400px;\n\t\tborder: 1px solid #ccc;\n\t}\n\t.show{\n\t\tcolor: red;\n\t}\n\t.mo{\n\t\twidth: 100%;\n\t\theight: 100%;\n\t\tposition: absolute;\n\t\ttop: 0px;\n\t\tleft: 0px;\n\t\tbackground: rgba(0,0,0,.9);\n\t}\n\t.footer{\n\t\tposition: absolute;\n\t\tbottom: 10px;\n\t\tleft: 0px;\n\t\tright: 0px;\n\t}\n</style>\n"]}]}